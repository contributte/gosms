{
  "name": "contributte/gosms",
  "description": "Full featured GoSMS.cz HTTP client for nette",
  "keywords": [
    "contributte",
    "gosms",
    "nette",
    "sms"
  ],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/contributte/gosms",
  "authors": [
    {
      "name": "Milan Felix Šulc",
      "homepage": "https://f3l1x.io"
    },
    {
      "name": "Filip Šuška",
      "email": "filipsuska@gmail.com"
    }
  ],
  "require": {
    "php": ">= 8.0",
    "ext-json": "*",
    "guzzlehttp/psr7": "^2.4.4",
    "nette/utils": "^3.2.9 || ^4.0.0",
    "psr/http-client": "^1.0"
  },
  "require-dev": {
    "contributte/guzzlette": "^3.3.0",
    "contributte/tester": "^0.3.0",
    "contributte/qa": "^0.4.0",
    "mockery/mockery": "^1.4.3",
    "nette/caching": "^3.2.2",
    "nette/di": "^3.1.2",
    "phpstan/phpstan": "^2.0.0",
    "phpstan/phpstan-deprecation-rules": "^2.0.0",
    "phpstan/phpstan-nette": "^2.0.0",
    "phpstan/phpstan-strict-rules": "^2.0.0"
  },
  "autoload": {
    "psr-4": {
      "Contributte\\Gosms\\": "src"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "suggest": {
    "contributte/guzzlette": "As default HttpClient",
    "nette/caching": "As cache handler in AccessTokenCacheProvider"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.5.x-dev"
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
